pipeline {
    agent any
    stages {        
        stage("parallel build") {
            steps {
                parallel (
                   "Start step1" : {
                       sh 'sudo docker run defdev/hello-world'
                    },
                    "Start step2" : {
                       sh 'sudo docker run -e ENV_VARIABLE=Riccardo defdev/hello-world-env'
                    }
                )
            }
        }
    }
}
